	RewriteCond %{REQUEST_FILENAME} !-d
	RewriteCond %{REQUEST_FILENAME} !-f

	RewriteRule ([a-z0-9-]+)/? http://$1.loveyouboo.de [R=301,NC,L]

	RewriteCond %{HTTP_HOST} ^([^.]*).(de|com|net|org/)
	RewriteRule ^.*$ http://www.%1.%2/G4/%{REQUEST_URI} [R=301,L]




