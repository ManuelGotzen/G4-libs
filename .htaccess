#	.	.	.	.	.	.	.	.	.	.	.	.	.	.	.	.	.	.	.	.	.	.	.	.	.	.	.	.	.	.	.	.	.
#
#	REMOVE INPUT FILTER DIRECTIVE
#
#	RemoveOutputFilter pdf
#	RemoveType .pdf
#
# 	RemoveInputFilter Directive
# 	
#	AddInputFilter			> > > http://httpd.apache.org/docs/2.0/mod/mod_mime.html#addinputfilter
#	SetInputFilter			> > > http://httpd.apache.org/docs/2.0/mod/core.html#setinputfilter
#	
#	<Directory /www/data/>
#		SetOutputFilter INCLUDES
#	</Directory> 
#


<FilesMatch ".(txt|rtf)$">
#	REMOVE INPUT FILTER DIRECTIVE
	RemoveOutputFilter txt rtf
	RemoveType .txt .rtf	
	RemoveInputFilter Directive	
	
# 	DECLARE SetOutputFilter	
	AddType application/x-httpd-php .txt .rtf

#	REMOVE HANDLER DIRECTIVE
	AddHandler server-parsed .txt .rtf
	AddType text/* .txt
	AddType text/plain .rtf	
	AddIconByType (RTF,http://loveyouboo.de/G4/_common/_assets/icons/dlf/text.gif) text/plain
	AddIconByType (TXT,http://oveyouboo.de/G4/_common/_assets/icons/dlf/sig.gif) text/*
	Header set content-type "text/plain; charset=utf-8"	
	Header append Vary User-Agent env=!dont-vary
</FilesMatch>

<FilesMatch ".(css)$">	
#	REMOVE INPUT FILTER DIRECTIVE	
	RemoveOutputFilter css
	RemoveType .css	
	RemoveInputFilter Directive	
	
# 	DECLARE SetOutputFilter	
#	AddType application/x-httpd-php .css
	AddType text/css .css

#	REMOVE HANDLER DIRECTIVE
	AddHandler server-parsed .css
	AddIconByType (CSS,http://loveyouboo.de/G4/_common/_assets/icons/dlf/css.png) text/css	
	Header set content-type "text/css; charset=ISO-8859-1"	
#	Header set content-type "text/plain; charset=utf-8"	
#	Header append Vary User-Agent env=!dont-vary
</FilesMatch>

#<FilesMatch "\.(x?html|css)$">
#	DECLARE SetOutputFilter
#	AddType text/html .css
#	Header append Vary User-Agent env=!dont-vary
#</FilesMatch>


# 	1 Month for all your static assets
<FilesMatch ".(ico|pdf|flv|jpg|jpeg|png|gif|js|css|swf)$">
	Header set Cache-Control "max-age=2592000, public"
</FilesMatch>

# 	1 DAYS for rss feeds and robots
<FilesMatch ".(xml|txt)$">
 	Header set Cache-Control "max-age=86400, public, must-revalidate"
</FilesMatch>
  
# 		4 HOURS for your real articles files
<FilesMatch ".(html|htm)$">
 	Header set Cache-Control "max-age=14400, must-revalidate"
	Header set X-UA-Compatible "IE=7"	
</FilesMatch>



